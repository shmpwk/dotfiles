	"icon": "resources/logo.png",
	"keywords": [
		"mermaid",
		"markdown",
		"flowchart",
		"diagram",
		"preview"
	],
	"publisher": "bierner",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/mjbvz/vscode-markdown-mermaid.git"
	},
